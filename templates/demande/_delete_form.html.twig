<form method="post" action="{{ path('app_demande_delete', {'iddemande': demande.iddemande}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ demande.iddemande) }}">
    <button class="btn btn-light" style="padding: 0.75rem 1rem;"><i class="bi bi-trash "></i></button>
</form>
