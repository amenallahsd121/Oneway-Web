{% extends 'backbase.html.twig' %}


{% block title %}
	Ajouter Livreur
{% endblock %}


{% block body %}


{{ form_start(form,{'attr': {'novalidate': 'novalidate'}}) }}

<div class="col-12 grid-margin stretch-card">
  <div class="card">
    <div class="card-body">
      <h4 class="card-title">Ajouter Livreur</h4>
      <p class="card-description">Veuillez saisir les details de Livreur :</p>

      <form class="forms-sample">
        <div class="form-group mt-3">
          {{ form_label(form.cinLivreur, "CIN") }}
          {{ form_widget(form.cinLivreur, {'attr': {'class': 'form-control'}}) }}
          {{ form_errors(form.cinLivreur) }}
        </div>

        <div class="form-group mt-3">
          {{ form_label(form.prenom, "Prenom") }}
          {{ form_widget(form.prenom, {'attr': {'class': 'form-control'}}) }}
          {{ form_errors(form.prenom) }}
        </div>

        <div class="form-group mt-3">
          {{ form_label(form.nom, "Nom") }}
          {{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
          {{ form_errors(form.nom) }}
        </div>

        <div class="form-group mt-3">
          {{ form_label(form.vehicule, "VÃ©hicule") }}
          {{ form_widget(form.vehicule, {'attr': {'class': 'form-control'}}) }}
          {{ form_errors(form.vehicule) }}
        </div>

          
        <div class="form-group mt-3">
          {{ form_widget(form.Ajouter) }}
           



        <a href="{{ path ('app_livreur') }}" class="btn btn-dark">Annuler</a>


      </form>
    </div>
  </div>
</div>

{{ form_end(form) }}

{% endblock %}
