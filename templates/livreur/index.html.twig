{% extends 'backbase.html.twig' %}


{% block title %}
	Livreur
{% endblock %}


{% block body %}



	 <div class="row ">
            <div class="col-12 grid-margin">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Liste des Livreurs</h4>
                  <div class="table-responsive">
                    <table class="table">
                      <thead>
                        <tr>
                          <th> CIN </th>
                          <th> Prénom </th>
                          <th> Nom </th>
                          <th> Véhicule </th>
                        </tr>
                      </thead>

                      {% for data in list %}
                        <tr>
                         <thead>
                          <td> {{ data.cinLivreur }} </td>
                          <td> {{ data.prenom }} </td>
                          <td> {{ data.nom }} </td>
                          <td> {{ data.vehicule }} </td>
                       
                          <td>
                            <div class="badge badge-outline-success">Approved</div>
                          </td>
                        </tr>
                          </thead>
                         {% endfor %}
                       
                         
                

{% endblock %}


