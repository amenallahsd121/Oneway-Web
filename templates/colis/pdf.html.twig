<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">

		{% block title %}
			<title>Details De Colis</title>
		{% endblock %}

		<!-- Bootstrap icons CSS -->
		{% block css %}
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.6.0/font/bootstrap-icons.css">
			<link rel="stylesheet" type="text/css" href="http://localhost/oneway/templates/colis/pdf.css">
		{% endblock %}

	</head>

	{% block body %}
		<body>
			<div class="container">
				<img src="data:image/png;base64," class="logo">
				{% for c in colis %}
					<h2>Merci Pour Votre Achat Cher client :
						{{ c.id_client.name }}
						{{ c.id_client.lastname }}
					</h2>
					<table class="info">
						<caption>Détails de Paiement :</caption>
						<thead>
							<tr>
								<th>Type Colis</th>
								<th>Poids</th>
								<th>Prix</th>
								<th>Lieu de départ</th>
								<th>Lieu d'arrivée</th>
							</tr>
						</thead>
						<tbody>

							<tr>
								<td>{{ c.typeColis }}
								</td>
								<td>{{ c.poids }}
									Kg</td>
								<td>{{ c.prix }}
									DT</td>
								<td>{{ c.lieuDepart }}</td>
								<td>{{ c.lieuArrive }}</td>
							</tr>
						{% endfor %}

					</tbody>

				</table>

				<img src="data:image/png;base64," class="car">
				<h5>
					Votre Colis Sera Livré Dans 24H
				</h5>
				<div class="rights-reserved">

					All rights reserved &copy; 2023
					<h4>
						-Oneway-
					</h4>

				</div>
			</div>

		</body>

	{% endblock %}
</html>
